  "private": true,
  "dependencies": {
    "bootstrap": "4.0.0-beta.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-yandex-maps": "^2.1.4",
    "styled-components": "^2.4.0"
  },
  "devDependencies": {
    "node-sass": "^4.7.2",
